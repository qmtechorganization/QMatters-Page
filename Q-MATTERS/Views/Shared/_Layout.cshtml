                    <div class="navbar-nav mx-0 mx-lg-auto">
                        <a asp-action="Index" asp-controller="Home" class="nav-item nav-link active">Home</a>
                        <a asp-action="Index" asp-controller="Inspection" class="nav-item nav-link">Inspection </a>
                        <a asp-action="Index" asp-controller="Services" class="nav-item nav-link">Training</a>
                        <a asp-action="Index" asp-controller="Liaison" class="nav-item nav-link">Liaison</a>
